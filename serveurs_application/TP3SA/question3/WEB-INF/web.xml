<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">


  <servlet>
    <servlet-name>SessionQuitter</servlet-name>
    <servlet-class>tp3sa.question1.SessionQuitter</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SessionEditor</servlet-name>
    <servlet-class>tp3sa.question1.SessionEditor</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SessionQuitter</servlet-name>
    <url-pattern>/quitsession</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SessionEditor</servlet-name>
    <url-pattern>/editsession</url-pattern>
  </servlet-mapping>
</web-app>
